# base test, should give lv 22 
#listRandom=(0)
#listAgent=(30)


listRandom=(0 25 50 75 100 125 150)
listAgent=(150 125 100 75 50 25 0) 

for i in ${!listRandom[@]}; do
  Rand=${listRandom[i]}
  Agent=${listAgent[i]}
  export AGENT_RANDOM_QUANTITY=$Rand
  export AGENT_TEAM3_QUANTITY=$Agent
  OUTPUT=$(make| tail -1)
  echo $Rand $Agent $OUTPUT
done
